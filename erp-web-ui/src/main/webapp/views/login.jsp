<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<meta http-equiv="Content-type" content="text/html; charset=utf-8" />
		<title>:: Login Page :: Warehouse Management System</title>
		<link rel="stylesheet" href="css/style.css" type="text/css" media="all" />
	</head>
	<body>

		<!-- Header -->
		<div id="header">
			<div class="shell">
				<!-- Logo + Top Nav -->
				<div id="top">
					<h1>Dynamic Text</h1>
					<div id="top-navigation">
						Welcome <a href="#"><strong>Administrator</strong></a>
						<span>|</span>				
						<a href="#">Profile Settings</a>
						<span>|</span>
						<a href="#">Log out</a>
					</div>
				</div>
				<!-- End Logo + Top Nav -->
				
				
			</div>
		</div>
		<!-- End Header -->

		<!-- Container -->
		<div id="container">
			<div class="shell">

				<!-- Main -->
				<div id="main">
					<div class="cl">&nbsp;</div>
					
					<!-- Content -->
					<div id="content">
						
						<!-- Box -->
						<div class="box">
							<!-- Box Head -->
							<div class="box-head">
								<h2 class="left">Login</h2>						
							</div>
							<!-- End Box Head -->

							<form action="" method="post">
						
								<!-- Form -->
								<div class="form">
									<p>							
										<label>User Name <span>(Required Field)</span></label>
										<input type="text" autocomplete="off" class="field size2" />
									</p>

									<p>										
										<label>Password <span>(Required Field)</span></label>
										<input type="password" autocomplete="off" class="field size2" />
									</p>	
									<p>
										<label>Client Type <span></span></label>
										<select class="field size3">
											<option value="Admin">Admin</option>
											<option value="Warehouse">Warehouse</option>
											<option value="Sales">Sales</option>
										</select>
									</p>								
									
								</div>
								<!-- End Form -->
								
								<!-- Form Buttons -->
								<div class="buttons">									
									<input type="submit" class="button" value="Login" />
								</div>
								<!-- End Form Buttons -->
							</form>

						</div>
					</div>
				</div>		

				<div class="cl">&nbsp;</div>

			</div>
		</div>

		<!-- Footer -->
		<div id="footer">
			<div class="shell">
				<span class="left">&copy; 2018</span>	
				<span class="right">Design by <a href="mailto:sumitmhatre25@gmail.com" target="_blank">Sumit Mhatre</a></span>
			</div>
		</div>
		<!-- End Footer -->

	</body>
</html>